#include <bits/stdc++.h>
using namespace std;

void solve() {
    int n; cin >> n;
    vector<long long> a(n);
    for(auto& it : a) cin >> it;
    long long curr = 0, uq = 0, sc = 0;
    vector<long long> ans;
    for(int i = 0; i < n; i++) {
        if(sc + i + 1 == a[i]) ans.push_back(++curr);
        else if(sc + 1 == a[i]) ans.push_back(ans.back()); 
        else ans.push_back(ans[sc+i-a[i]]);
        sc = a[i];
    }
    for(auto& it : ans) cout << it << " "; cout << endl; 
}

int main() {
	int t; cin >> t;
	while(t--) solve();

}